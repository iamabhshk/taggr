    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
